import java.util.*;

public class Test_Kattis {
    
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);

        int Wc = sc.nextInt(), Hc = sc.nextInt(), Ws = sc.nextInt(), Hs = sc.nextInt();
        
        if(Wc >= Ws+2 && Hc >= Hs+2) {
            System.out.println(1);
        }
        else {
            System.out.println(0);
        }
        
        sc.close();
    }
}
