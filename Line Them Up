#include <iostream>
#include <vector>
#include <string>
#include <unordered_map>

using namespace std;

int main()
{
    string name;
    int n;

    cin >> n ;
    vector<string> TableName;

    for(int i=0; i<n; i++) {
        cin >> name;
        TableName.push_back(name) ;
    }

    bool D=false, I=false;

    for (int i=0; i<TableName.size()-1; i++) {
        if (TableName[i].compare(TableName[i+1]) < 0) {
            I = true;
        }
        else {
            I = false;
            break;
        }
    }

    for (int i=0; i<n-1; i++) {
        if (TableName[i].compare(TableName[i+1]) > 0) {
            D = true;
        }
        else {
            D = false;
            break;
        }
    }

    if(!D && !I) {
        cout << "NEITHER" ;
    }
    else if (D) {
        cout << "DECREASING" ;
    }
    else if (!D && I) {
        cout << "INCREASING" ;
    }

    return 0;
}
